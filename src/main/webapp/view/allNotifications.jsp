	<script type="text/javascript">
	$(".mark-as-read").click(function(){
		var notifid=this.id;
		$.ajax({
			url:"changeNotifStatus?notif_id="+notifid,
			type:"get",
			success:function(response){
				if(response){
					$("#"+notifid).removeClass("mark-as-read");
					$("#"+notifid).html('<i class="fa fa-check" aria-hidden="true" style="color:#44f544;"></i>');
					$("#"+notifid).parents('.notif').removeClass('unread');
					$("#not"+notifid).removeClass('unread');
					var count=$("#noOfNotif").text();
					if(count>0){
						count-=1;
						$("#noOfNotif").text(count);
						$("#notif_header_text").text("You have "+count+" unread notification(s)");
					}
					if(count==0){
						$("#noOfNotif").hide();
						$("#notif_header_text").text("You don't have any new notifications.");
					}
				}
			},
			error:function(err){
				console.log(err);
			}
		});
	});
	
